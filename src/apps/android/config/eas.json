{
	"cli": {
		"version": ">= 16.4.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "dev"
			},
			"android": {
				"buildType": "apk"
			}
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "preview"
			},
			"android": {
				"buildType": "apk"
			}
		},
		"production": {
			"autoIncrement": true,
			"android": {}
		}
	},
	"submit": {
		"production": {}
	}
}
