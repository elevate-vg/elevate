{
   "name": "@elevate/api",
   "version": "1.0.0",
   "scripts": {
      "dev": "nodemon",
      "webpack": "webpack",
      "build": "npm run build:tsc && npm run build:next",
      "build:tsc": "cross-env NODE_ENV=production tsc --project tsconfig.server.json",
      "build:next": "cross-env NODE_ENV=production next build",
      "package": "mkdir -p ../../dist/api && cp -R .next dist/* ../../dist/api && package:install",
      "package:install": "npm install --prefix ../../dist/apps/api",
      "start": "cross-env NODE_ENV=production node dist/index.js"
   },
   "dependencies": {
      "cross-env": "^7.0.2",
      "express": "^4.17.1",
      "puppeteer": "^10.0.0"
   },
   "devDependencies": {
      "@types/express": "^4.17.12",
      "@types/node": "^15.12.4",
      "@typescript-eslint/eslint-plugin": "^4.27.0",
      "@typescript-eslint/parser": "^4.27.0",
      "eslint": "^7.29.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-cypress": "^2.11.3",
      "eslint-plugin-jest": "^24.3.6",
      "eslint-plugin-prettier": "^3.4.0",
      "express-static-gzip": "^2.1.1",
      "nodemon": "^2.0.7",
      "prettier": "^2.3.1",
      "ts-loader": "^9.2.3",
      "ts-node": "^10.0.0",
      "typescript": "4.3.4",
      "webpack": "^5.40.0",
      "webpack-cli": "^4.7.2",
      "webpack-node-externals": "^3.0.0"
   },
   "license": "MIT"
}
