{
	"name": "elevate",
	"version": "1.0.0",
	"main": "src/apps/android/index.ts",
	"scripts": {
		"dev": "mprocs --names expo,vite,webview \"npm run android::serve\" \"npm run ui::watch\" \"npm run webview::string::watch\"",
		"android::serve": "expo start",
		"android::build": "npm run android::build::preview && npm run android::build::development",
		"android::build::preview": "npm run ui::build && eas build --platform android --profile preview --local --output ./builds/elevate-preview-$(date +%Y%m%d-%H%M%S).apk",
		"android::build::development": "npm run ui::build && eas build --platform android --profile development --local --output ./builds/elevate-development-$(date +%Y%m%d-%H%M%S).apk",
		"ui::watch": "vite build --watch",
		"ui::build": "vite build && npm run webview::string",
		"ui::serve": "vite dev",
		"webview::string": "node scripts/build-webview-string.js",
		"webview::string::watch": "chokidar \"src/apps/android/generated/index.html\" -c \"npm run webview::string\""
	},
	"dependencies": {
		"@elasticbottle/trpc-post-message": "^0.0.4",
		"@noriginmedia/norigin-spatial-navigation": "^2.3.0",
		"@trpc/client": "^11.3.1",
		"@trpc/server": "^11.3.1",
		"@types/js-yaml": "^4.0.9",
		"expo": "53.0.11",
		"expo-asset": "^11.1.5",
		"expo-dev-client": "~5.2.0",
		"expo-file-system": "^18.1.10",
		"expo-intent-launcher": "~12.1.5",
		"expo-keep-awake": "^14.1.4",
		"expo-navigation-bar": "^4.2.5",
		"expo-status-bar": "~2.2.3",
		"js-yaml": "^4.1.0",
		"react": "19.0.0",
		"react-native": "0.79.3",
		"react-native-edge-to-edge": "^1.6.1",
		"react-native-webview": "13.13.5",
		"superjson": "^2.2.2",
		"zod": "^3.25.56"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@types/react": "~19.0.10",
		"@types/react-dom": "*",
		"@vitejs/plugin-react": "^4.0.0",
		"chokidar-cli": "^3.0.0",
		"react-dom": "19.0.0",
		"typescript": "~5.8.3",
		"vite": "^6.3.5",
		"vite-plugin-singlefile": "^2.2.0"
	},
	"private": true
}
